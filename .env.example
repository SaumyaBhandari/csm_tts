# CSM TTS Environment Configuration
# Copy this file to .env and customize

# HuggingFace token (required for model access)
HF_TOKEN=your_huggingface_token_here

# Private watermark key (5 comma-separated integers)
# Generate your own unique key for traceability
# Example: CSM_WATERMARK_KEY=123,456,789,101,202
CSM_WATERMARK_KEY=

# Enable watermarking by default (true/false)
CSM_WATERMARK_ENABLED=true

# Server port
PORT=8000
