# CSM TTS Configuration
# Copy to .env and customize

# Server
HOST=0.0.0.0
PORT=8000

# Model device (auto, cuda, cpu)
MODEL_DEVICE=auto

# HuggingFace token (for model access)
# HF_TOKEN=your_token_here

# Watermark
CSM_WATERMARK_ENABLED=true
CSM_WATERMARK_KEY=

# Generation defaults
DEFAULT_SPEAKER_ID=0
DEFAULT_MAX_AUDIO_MS=30000
DEFAULT_TEMPERATURE=0.9
DEFAULT_TOPK=50
